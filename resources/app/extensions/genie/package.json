    "@types/mocha": "^10.0.1",
    "@types/vscode": "^1.70.2",
    "@vscode/test-electron": "^2.5.2",
    "archiver": "^5.3.2",
    "cross-env": "^7.0.3",
    "glob": "^7.1.4",
    "mocha": "^10.2.0"
  },
